<div class="container">
  <div class="row">
    <div class="col-sm-10">
      <h1>Entradas mas recientes!</h1>
      <div class="card">
        <div class="card-body">
          <% @posts.each do |post| %>
            <h2 class="card-title"><%= post.title %></h2>
            <p class="card-text"><%= post.body %></p>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>

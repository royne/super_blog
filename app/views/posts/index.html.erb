<div class="container">
  <div class="row">
    <div class="col-sm-10">
      <h1>Entradas mas recientes!</h1>
      <% @posts.each do |post| %>
        <div class="card">
          <div class="card-header"><strong><%= post.title %></strong></div>
          <div class="card-body">
            <h4 class="card-title"><%= link_to post.title, post %></h4>
            <p class="card-text"><%= post.body %></p>
          </div>
        </div>
        <br>
      <% end %>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-sm-10">
      <h1>Entradas mas recientes!</h1>
      <div class="card">
        <% @posts.each do |post| %>
        <div class="card-header"><strong><%= post.title %></strong></div>
          <div class="card-body">
            <h4 class="card-title"><%= post.title %></h4>
            <p class="card-text"><%= post.body %></p>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
